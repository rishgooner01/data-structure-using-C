#include<stdio.h>
#include<stdlib.h>
int main()
{
    int m,n;
    scanf("%d %d\n",&m,&n);
    int a[m][n],b[m][n],c[m][n];
    int i,j;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d\n",&a[i][j]);
            scanf("%d\n",&b[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            c[i][j]=a[i][j]-b[i][j];
            printf("%d ",c[i][j]);
        }
    }
    return 0;
}
